  <application class="gate.groovy.ScriptableControllerPersistence">
    <controlScript class="string">import gate.termraider.*;
def tfIdfCopierPR = prs.find { it.name == &quot;tfIdfCopier&quot; }
def augTfIdfCopierPR = prs.find { it.name == &quot;augTfIdfCopier&quot; }
def kyotoCopierPR = prs.find { it.name == &quot;kyotoCopier&quot; }
String instNamespace = gate.termraider.output.OntologyGenerator.generateInstanceNamespace();

// equivalent to old preprocessor
eachDocument {
    docReset();
    tokenizer();
    //stopwords();
    splitter();
    //textCat();
    annieGazetteer();
    posTagger();
    lemmatizer();
    annieJape();
    orthomatcher();
    //stopwordsJape();
    selectTokens();
    multiwordJape();
    deduplicateMW();
    applyLanguage();
}

// create the TfIdfTermbank LR
String termbankName0 = &quot;gate.termraider.bank.TfIdfTermbank&quot;;
FeatureMap parameters0 = Factory.newFeatureMap();
Set&lt;Corpus&gt; corporaSL = Collections.singleton(corpus);
Set&lt;String&gt; inputTypes = new HashSet&lt;String&gt;();
inputTypes.add(&quot;SingleWord&quot;);
inputTypes.add(&quot;MultiWord&quot;);
parameters0.put(&quot;corpora&quot;, corporaSL);
parameters0.put(&quot;inputAnnotationTypes&quot;, inputTypes);
parameters0.put(&quot;debugMode&quot;, true);
parameters0.put(&quot;namespaceBase&quot;, instNamespace);
LanguageResource termbank0 = (LanguageResource) Factory.createResource(termbankName0, parameters0);

tfIdfCopierPR.setParameterValue(&quot;termbank&quot;, termbank0);
eachDocument {
    tfIdfCopier();
    augmentation();
}
tfIdfCopierPR.setParameterValue(&quot;termbank&quot;, null);

// create the AugmentedTermbank LR
String termbankName1 = &quot;gate.termraider.bank.AnnotationTermbank&quot;;
FeatureMap parameters1 = Factory.newFeatureMap();
parameters1.put(&quot;corpora&quot;, corporaSL);
parameters1.put(&quot;inputAnnotationTypes&quot;, inputTypes);
parameters1.put(&quot;debugMode&quot;, true);
parameters1.put(&quot;inputScoreFeature&quot;, &quot;localAugTfIdf&quot;);
parameters1.put(&quot;namespaceBase&quot;, instNamespace);

LanguageResource termbank1 = (LanguageResource) Factory.createResource(termbankName1, parameters1);

// create the HyponymyTermbank LR
String termbankName2 = &quot;gate.termraider.bank.HyponymyTermbank&quot;;
FeatureMap parameters2 = Factory.newFeatureMap();
List&lt;String&gt; t2features = new ArrayList&lt;String&gt;();
t2features.add(&quot;head&quot;);
t2features.add(&quot;canonical&quot;);
parameters2.put(&quot;inputHeadFeatures&quot;, t2features);
parameters2.put(&quot;inputAnnotationTypes&quot;, inputTypes);
parameters2.put(&quot;inputAnnotationFeature&quot;, &quot;canonical&quot;);
parameters2.put(&quot;corpora&quot;, corporaSL);
parameters2.put(&quot;debugMode&quot;, true);
parameters2.put(&quot;namespaceBase&quot;, instNamespace);

LanguageResource termbank2 = (LanguageResource) Factory.createResource(termbankName2, parameters2);

augTfIdfCopierPR.setParameterValue(&quot;termbank&quot;, termbank1);
kyotoCopierPR.setParameterValue(&quot;termbank&quot;, termbank2);
eachDocument {
    augTfIdfCopier();
    kyotoCopier();
}
augTfIdfCopierPR.setParameterValue(&quot;termbank&quot;, null);
kyotoCopierPR.setParameterValue(&quot;termbank&quot;, null);
</controlScript>
