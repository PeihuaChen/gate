		<CLASS>gate.gui.GazetteerEditor</CLASS>
		<COMMENT>Gazetteer viewer and editor.</COMMENT>
		<HELPURL>http://gate.ac.uk/userguide/sec:gazetteers:anniegazeditor</HELPURL>
		<GUI TYPE="large">
		<MAIN_VIEWER />
