<?xml version="1.0"?>
<!-- gate.xml -->
<!-- $Id$ -->

<!-- any entries in this file will be processed at Gate.init()
     time - i.e. will appear in every GATE invocation -->

<GATE>

<GATECONFIG Save_options_on_exit="true" Save_session_on_exit="true"/>

<SYSTEM NAME="Brings into GATE the default annotation schemas">
  <CONTROLLER>none</CONTROLLER>
  <LR xmlFileUrl="gate:/creole/schema/AddressSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/DateSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/IdentifierSchema.xml">gate.creole.AnnotationSchema</LR>
<LR xmlFileUrl="gate:/creole/schema/GpeSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/LocationSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/MoneySchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/OrganizationSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/PercentSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/PersonSchema.xml">gate.creole.AnnotationSchema</LR>
  <LR xmlFileUrl="gate:/creole/schema/SyntaxTreeNodeSchema.xml">gate.creole.AnnotationSchema</LR>
</SYSTEM>

</GATE>
