      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE="gate:/creole/schema/DateSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/FacilitySchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE ="gate:/creole/schema/GpeSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/IdentifierSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE ="gate:/creole/schema/PersonSchema.xml" 
          />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/LocationSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE ="gate:/creole/schema/MoneySchema.xml" 
          />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/OrganizationSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/PercentSchema.xml" />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE ="gate:/creole/schema/PersonSchema.xml" 
          />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE ="gate:/creole/schema/TokenSchema.xml" 
          />
      </AUTOINSTANCE>
      <AUTOINSTANCE>
        <PARAM NAME ="xmlFileUrl" VALUE 
          ="gate:/creole/schema/SyntaxTreeNodeSchema.xml" />
      </AUTOINSTANCE>
