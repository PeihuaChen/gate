    <RESOURCE>
      <NAME>Document Reset PR</NAME>
      <CLASS>gate.creole.annotdelete.AnnotationDeletePR</CLASS>
      <COMMENT>Document cleaner</COMMENT>
      <PARAMETER NAME="document" RUNTIME="true">gate.Document</PARAMETER>
      <PARAMETER NAME="annotationTypes" RUNTIME="true" OPTIONAL="true" 
        ITEM_CLASS_NAME="java.lang.String"> java.util.ArrayList</PARAMETER>
      <PARAMETER NAME="setsToKeep" RUNTIME="true" OPTIONAL="true" 
        ITEM_CLASS_NAME="java.lang.String"> java.util.ArrayList</PARAMETER>
    </RESOURCE>
    <RESOURCE>
      <NAME>Flexible Exporter</NAME>
      <CLASS>gate.creole.dumpingPR.DumpingPR</CLASS>
      <COMMENT>Exports a document with GATE annotations to its original 
        format</COMMENT>
      <PARAMETER NAME="document" RUNTIME="true">gate.Document</PARAMETER>
      <PARAMETER NAME="annotationSetName" RUNTIME="true" OPTIONAL="true">
        java.lang.String</PARAMETER>
      <PARAMETER NAME="useStandOffXML" OPTIONAL="true" DEFAULT="false" 
        COMMENT="Whether to export to stand-off XML rather than inline annotations">
         java.lang.Boolean</PARAMETER>
      <PARAMETER NAME="annotationTypes" RUNTIME="true" 
        DEFAULT="Person;Location;Date">java.util.ArrayList</PARAMETER>
      <PARAMETER NAME="dumpTypes" RUNTIME="true" OPTIONAL="true" 
        DEFAULT="Person;Location;Date" 
        COMMENT="A list of the new names to be used instead of the ANNIE types given for dumping">
        java.util.ArrayList</PARAMETER>
      <PARAMETER NAME="outputFileUrl" RUNTIME="true" OPTIONAL="true" 
        COMMENT="The url of the output file" SUFFIXES="xml;html;txt">
        java.net.URL</PARAMETER>
      <PARAMETER NAME="includeFeatures" OPTIONAL="true" 
        COMMENT="Whether or not to include features when exporting the GATE annotations" 
        DEFAULT="false">java.lang.Boolean</PARAMETER>
      <PARAMETER NAME="useSuffixForDumpFiles" OPTIONAL="true" 
        COMMENT="Whether or not to use a suffix for the dump files" 
        DEFAULT="true">java.lang.Boolean</PARAMETER>
      <PARAMETER NAME="suffixForDumpFiles" OPTIONAL="true" 
        COMMENT="The suffix used for files, if suffix adding is set to true" 
        DEFAULT=".gate">java.lang.String</PARAMETER>
    </RESOURCE>
    <!-- creole.xml for the DAML+OIL exporter -->
    <RESOURCE>
      <NAME>DAML+OIL exporter</NAME>
      <CLASS>gate.creole.RDFFormatExporter</CLASS>
      <COMMENT>A DAML+OIL exporter</COMMENT>
      <PARAMETER NAME="document" COMMENT="The document to be exported" 
        RUNTIME="true">gate.corpora.DocumentImpl</PARAMETER>
      <PARAMETER NAME="ontology" COMMENT="Ontology" SUFFIXES="daml;rdf">
        java.net.URL</PARAMETER>
      <PARAMETER DEFAULT="Organization;Person;Location;Facility;GPE" 
        NAME="exportedTypes">java.util.ArrayList</PARAMETER>
      <PARAMETER NAME="exportFilePath" 
        COMMENT="Destination URL where to export" SUFFIXES="daml;rdf">
        java.net.URL</PARAMETER>
      <PARAMETER DEFAULT="DAML+OIL" NAME="exportFormat">
        java.lang.String</PARAMETER>
      <!-- new Parameter added by Niraj -->
      <PARAMETER NAME="annotationSetName" 
        COMMENT="AnnotationSet that should be exported" RUNTIME="true" 
        OPTIONAL="true">java.lang.String</PARAMETER>
      <ICON>daml.jpg</ICON>
    </RESOURCE>
    <!-- creole.xml for SearchPR -->
    <RESOURCE>
      <NAME>SearchPR</NAME>
