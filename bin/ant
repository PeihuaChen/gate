#!/bin/sh

# When CDPATH is set, the cd command prints out the current directory, which 
# breaks ANT's routine for finding ANT_HOME.
# Because of this, ANT_HOME is now set by the gate.sh script.
#unset ANT_HOME

dir=`dirname $0`

if [ -x "$dir/real-ant" ]; then
  exec $dir/real-ant --noconfig "$@"
else
  echo "Couldn't find real-ant"
fi
